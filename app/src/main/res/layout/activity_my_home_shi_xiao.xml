<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_home_shi_xiao"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zl.cn.jer.easycar.activity.myyikai.MyHomeShiXiaoActivity">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:background="#F3F8FC"
        >
        <ImageView
            android:id="@+id/img_back_shixiao"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_navi_back_dark"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="15dp"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="失效"
            android:gravity="center"
            android:textSize="18sp"
            android:layout_marginRight="50dp"
            />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/divider"
        ></View>

    <com.zl.cn.jer.easycar.view.xlistview.XListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        ></com.zl.cn.jer.easycar.view.xlistview.XListView>
</LinearLayout>

</RelativeLayout>
